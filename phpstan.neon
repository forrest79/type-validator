			count: 1
			path: src/TypeValidator/PHPStan/Type/ReturnTypeExtension.php

		-
			rawMessage: 'Using (object) is discouraged, please avoid using that.'
			identifier: shipmonk.forbiddenCast
			count: 2
			path: tests/PHPStanTest.php

