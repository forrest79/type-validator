includes:
	- %rootDir%/conf/bleedingEdge.neon
	- %rootDir%/../phpstan-strict-rules/rules.neon
	- %rootDir%/../../../vendor/shipmonk/phpstan-rules/rules.neon

	# Settings for tests
	- extension.neon

parameters:
	level: max
	excludePaths:
		- src/functions.php # isType() must be called only with constant string as $type, not a variable
		- tests/assets/incorrect.php # intentionally bad syntax
